                "downstream_tasks": ["stg-to-raw-agiloft-clauses"]
            },
            {
                "task_name": "stg-to-raw-agiloft-clauses",
                "task_type": "MERGE",
                "template_reference": "RAW_DL_UPDATE",
                "config": {
                    "stg_table": "helix-data-dev.helix_agiloft_stg.clauses",
                    "dl_table": "helix-data-dev.helix_agiloft_dl.clauses",
                    "raw_table": "helix-data-dev.helix_agiloft_raw.clauses",
