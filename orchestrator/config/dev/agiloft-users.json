
{
    "dag_config": {
        "dag_id": "legal_analytics-users",
        "schedule_interval": null,
        "sla_in_minutes": 180,
        "email": ["sherin@gmail.com"]
    },
    "task_config": {
        "airflow_variable_id_for_impersonation_chain": "legal-analytics-impersonation-chain",
        "data_project_id": "smart-pr",
        "tasks": [
            {
                "task_name": "fetch-from-source-users",
                "task_type": "EXECUTION",
                "template_reference": "LEGAL_ANALYTICS_CF",
                "config": {
                    "table": "users",
                    "gcs_path": "gs://legal-analytics-data/ingestion/users",
                    "client_id": "ah91bh709ldright1",
                    "secret_key": "legal_analytics-dev-secret",
                    "columns": "user_id, company_id, username, email_address, first_name, last_name, role, department, is_active, created_timestamp, last_login_timestamp",
                    "api_endpoint": "https://mycompany.com/legal_analytics/tables/users"
                },
                "downstream_tasks": ["gcs2bq-legal_analytics-users"]
            },
            {
                "task_name": "gcs2bq-legal_analytics-users",
                "task_type": "EXECUTION",
                "template_reference": "GCS2BQ",
                "config": {
                    "bucket": "legal-analytics-data",
                    "source_objects": [
                        "ingestion/users/*.json"
                    ],
                    "schema_object_bucket": "legal-analytics-configs",
                    "schema_object": "schemas/users.json",
                    "destination_project_dataset_table": "smart-pr.legal_analytics_stg.users"
                },
                "downstream_tasks": ["stg-to-raw-legal_analytics-users"]
            },
            {
                "task_name": "stg-to-raw-legal_analytics-users",
                "task_type": "MERGE",
                "template_reference": "RAW_DL_UPDATE",
                "config": {
                    "stg_table": "smart-pr.legal_analytics_stg.users",
                    "dl_table": "smart-pr.legal_analytics_dl.users",
                    "raw_table": "smart-pr.legal_analytics_raw.users",
                    "primary_key_columns": "clause_id"
                }
            }
        ]
    },
    "temp;ate_config": {
        "merge_templates": [
            {
                "template_id": "RAW_DL_UPDATE",
                "merge_type": "MERGE_UPDATE",
                "config": {
                    "truncate_stg_table": "False"
                }
            }
        ],

        "execution_templates": [
            {
                "template_id": "LEGAL_ANALYTICS_CF",
                "execution_type": "AUTHENTICATED_CLOUD_FUNCTION",
                "config": {
                    "method": "POST",
                    "url": "https://us-west1-smart-pr.cloudfunction.net/legal_analytics-ingestion",
                    "headers": {
                        "Content-Type": "application/json"
                    }
                }
            }
        ]
    }
